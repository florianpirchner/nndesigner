<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="athene" version="12.0.0.2017041100">
  <ownedViewpoints name="Athene NN Designer" modelFileExtension="athene">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="NNDesigner" domainClass="core::Design">
      <defaultLayer name="Default">
        <toolSections/>
      </defaultLayer>
      <additionalLayers name="Layers" activeByDefault="true">
        <nodeMappings name="Activation" domainClass="keras::Activation">
          <style xsi:type="style:WorkspaceImageDescription" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.eclipse.athene.sirius.spec/icons/man.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Dropout" domainClass="keras::Dropout"/>
        <nodeMappings name="Flatten" domainClass="keras::Flatten"/>
        <nodeMappings name="Permute" domainClass="keras::Permute"/>
        <nodeMappings name="RepeatVector" domainClass="keras::RepeatVector"/>
        <nodeMappings name="Reshape" domainClass="keras::Reshape"/>
        <edgeMappings name="connectedNode" sourceMapping="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense']/@borderedNodeMappings[name='inputNode']" targetMapping="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense']/@borderedNodeMappings[name='outputNode']" targetFinderExpression="feature:connectedNode">
          <style lineStyle="dash" targetArrow="NoDecoration" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="Dense" domainClass="keras.Dense">
          <borderedNodeMappings name="inputNode" deletionDescription="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@toolSections.0/@ownedTools[name='DeleteNode']" semanticCandidatesExpression="feature:inputNodes" domainClass="keras.InputNode">
            <style xsi:type="style:DotDescription" labelPosition="node">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <forbiddenSides>SOUTH</forbiddenSides>
              <forbiddenSides>EAST</forbiddenSides>
              <forbiddenSides>NORTH</forbiddenSides>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </borderedNodeMappings>
          <borderedNodeMappings name="outputNode" deletionDescription="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@toolSections.0/@ownedTools[name='DeleteNode']" semanticCandidatesExpression="feature:outputNodes" domainClass="keras.OutputNode">
            <style xsi:type="style:DotDescription" labelPosition="node">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <forbiddenSides>WEST</forbiddenSides>
              <forbiddenSides>SOUTH</forbiddenSides>
              <forbiddenSides>NORTH</forbiddenSides>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </containerMappings>
        <toolSections>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="createActivation" label="Activation" nodeMappings="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@nodeMappings[name='Activation']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="keras::Activate" referenceName="elements">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="New Layer"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="connectToInput" label="Tensor" edgeMappings="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Tensor']/@edgeMappings[name='Tensor']" iconPath="/org.eclipse.sirius.sample.basicfamily.advanced.design/icons/father.png">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="service:connectTensorToInput(target)"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="Dense" containerMappings="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="keras.Dense" referenceName="elements"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:DeleteElementDescription" name="DeleteNode">
            <element name="element"/>
            <elementView name="elementView"/>
            <containerView name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="service:removeNode()"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </additionalLayers>
      <additionalLayers name="Tensor" activeByDefault="true">
        <edgeMappings name="Tensor" deletionDescription="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Tensor']/@toolSections.0/@ownedTools[name='DeleteTensor']" sourceMapping="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense']/@borderedNodeMappings[name='outputNode']" targetMapping="//@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense'] //@ownedViewpoints[name='Athene%20NN%20Designer']/@ownedRepresentations[name='NNDesigner']/@additionalLayers[name='Layers']/@containerMappings[name='Dense']/@borderedNodeMappings[name='inputNode']" targetFinderExpression="[thisEObject.consumingNodes/]" sourceFinderExpression="[thisEObject.containerNode/]" domainClass="core::Tensor" useDomainElement="true">
          <style sizeComputationExpression="2" routingStyle="manhattan">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections>
          <ownedTools xsi:type="tool:DeleteElementDescription" name="DeleteTensor">
            <element name="element"/>
            <elementView name="elementView"/>
            <containerView name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="service:unconnectFromTargetInput(elementView)"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </additionalLayers>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="org.eclipse.athene.sirius.spec.Services"/>
  </ownedViewpoints>
</description:Group>
